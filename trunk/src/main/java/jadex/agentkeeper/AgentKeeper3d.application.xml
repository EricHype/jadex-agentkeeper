						<env:object3d width="0.79998" height="1" depth="0.79998" x="0" y="0" z="0" 
						modelpath="models/tilesets/claimed/Claimedpath.j3o" 
						materialpath="models/buildings/hatchery/HatcheryPlanks.j3m" />
						
						<env:object3d width="1" height="1" depth="1" x="0" y="0" z="0" 
							modelpath="models/tilesets/buildings/border/Default_?.j3o"
							materialpath="models/tilesets/buildings/border/default.j3m">
						</env:object3d>
						
						<env:object3d width="0.65" height="0.65" depth="0.65" x="0" y="0" z="0" 
						modelpath="models/buildings/hatchery/farm.j3o" 
						materialpath="models/buildings/hatchery/farm.j3m"
						rotation="rota" >
						<env:drawcondition>
							((ACenterBuildingInfo)$object.getProperty("tileInfo")).getCenterType() == CenterType.CENTER
						</env:drawcondition>
						</env:object3d>
					</env:drawable3d>
					
					<env:drawable3d objecttype="TRAININGROOM" width="1.0" height="1.0" depth="1.0" isDynamic="false">
					<env:object3d width="0.79998" height="1" depth="0.79998" x="0" y="0" z="0" 
						modelpath="models/tilesets/claimed/Claimedpath.j3o" 
						materialpath="models/buildings/training/TrainingGround.j3m"
						 />				
						 
						 
						 <env:object3d width="1" height="1" depth="1" x="0" y="0" z="0" 
							modelpath="models/tilesets/buildings/border/Default_?.j3o"
							materialpath="models/tilesets/buildings/border/default.j3m">
						</env:object3d>
						
						<env:object3d width="0.3" height="0.3" depth="0.3" x="0" y="0" z="0" 
						modelpath="models/buildings/training/trainingpole.j3o" 
						materialpath="models/buildings/training/trainingpole.j3m" 
						rotation="rota" >
						<env:drawcondition>
							((ACenterBuildingInfo)$object.getProperty("tileInfo")).getCenterType() == CenterType.CENTER
						</env:drawcondition>
						</env:object3d>	
						
					</env:drawable3d>
					
					
					<!-- 
					
						CREATURES
						
						
					 -->


					<env:drawable3d objecttype="imp" width="1" height="1"
						depth="1" isDynamic="true">
					<env:quad color="#00FF00" width="0.75" height="0.04" depth="0.04" x="0" y="2.0" z="0">
						<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
					</env:quad>
						<env:property name="level" dynamic="true">$object.getProperty("level").toString()
						</env:property>
						<env:property name="lifesize" dynamic="true">new
							Vector2Double(((((((Number)$object.getProperty("leben"))).doubleValue()/(((Number)$object.getProperty("maxleben"))).doubleValue()))*0.7),0.05)
						</env:property>
						<env:property name="lifeposition" dynamic="true">new
							Vector2Double((((($object.getProperty("leben")).doubleValue()/($object.getProperty("maxleben")).doubleValue())*0.70)/2)-0.9,-1)
						</env:property>
						<env:object3d width="0.15" height="0.15" depth="0.15"
							x="0" y="0" z="0" modelpath="models/kobold/kobold.j3o"
							materialpath="models/kobold/koboldmat.j3m"
							hasLightMaterials="true">
							<env:animation name="Walk" channel="chanA" loop="true" speed="4">
								<env:animationcondition>$object.getProperty("status").equals("Walk")</env:animationcondition>
							</env:animation>
							<env:animation name="Dance.001" channel="chanA" loop="true" speed="0.75">
								<env:animationcondition>$object.getProperty("status").equals("Idle")</env:animationcondition>
							</env:animation>
							
							<env:animation name="Idle" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Sleeping")</env:animationcondition>
							</env:animation>
							<env:animation name="Dig" channel="chanA" loop="true" speed="4">
								<env:animationcondition>$object.getProperty("status").equals("Dig")</env:animationcondition>
							</env:animation>
						</env:object3d>
						
						<env:sound3d soundfile="sounds/digslow/Digging?.ogg" numRndFiles="17" loop="false" volume="2.0" continuosly="true" positional="true"  >
						<env:soundcondition>
							$object.getProperty("status").equals("Dig")
						</env:soundcondition>
					</env:sound3d>
					</env:drawable3d>
					

					
					
					<env:drawable3d objecttype="thief" width="1" height="1" depth="1" isDynamic="true">

					<env:quad color="#00FF00" width="0.75" height="0.04" depth="0.04" x="0" y="2.0" z="0">
						<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
					</env:quad>
						<env:object3d 
						width="1" height="1" depth="1"
						x="0" y="0" z="0"
						modelpath="models/heroes/thief/Adventurer.j3o"
						>
						
							<env:animation name="Walk" channel="chanA" loop="true" speed="2">
								<env:animationcondition>$object.getProperty("status").equals("Walk")</env:animationcondition>
							</env:animation>
							
							<env:animation name="Idle" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Idle")</env:animationcondition>
							</env:animation>
							
							<env:animation name="Attack1" channel="chanA" loop="true" speed="1">
								<env:animationcondition>$object.getProperty("status").equals("Dance")</env:animationcondition>
							</env:animation>
							
						</env:object3d>
						
					</env:drawable3d>
					
					
					<env:drawable3d objecttype="warlock" width="1" height="1" depth="1" isDynamic="true">
					
					<env:quad color="#00FF00" width="0.75" height="0.04" depth="0.04" x="0" y="2.0" z="0">
						<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
					</env:quad>
					
					<env:quad color="#00FFFF" size="awakesize" x="0" y="1.9" z="0">
						<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
					</env:quad>
					<env:quad color="#FFDEAD" size="fedsize" x="0" y="1.85" z="0">
						<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
					</env:quad>
					
						<env:object3d 
						width="0.06" height="0.06" depth="0.06"
						x="0" y="1.1" z="0"
						modelpath="jadex3d/models/characters/medieval/mage/mage.j3o"
						hasLightMaterials="true">
						
							<env:animation name="Walk" channel="chanA" loop="true" speed="1.5">
								<env:animationcondition>$object.getProperty("status").equals("Walk")</env:animationcondition>
							</env:animation>
							
							<env:animation name="Idle" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Idle")</env:animationcondition>
							</env:animation>
							
							<env:animation name="Idle" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Sleeping")</env:animationcondition>
							</env:animation>
							
							<env:animation name="Hit" channel="chanA" loop="true" speed="0.1">
								<env:animationcondition>$object.getProperty("status").equals("Dance")</env:animationcondition>
							</env:animation>
							
						</env:object3d>
						
					</env:drawable3d>
					

					<env:drawable3d objecttype="goblin" width="1"
						height="1" depth="1">					
						<env:quad color="#00FF00" width="0.75" height="0.04" depth="0.04" x="0" y="2.0" z="0">
							<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
						</env:quad>
					
						<env:quad color="#00FFFF" size="awakesize" x="0" y="1.9" z="0">
							<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
						</env:quad>
						<env:quad color="#FFDEAD" size="fedsize" x="0" y="1.85" z="0">
							<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
						</env:quad>
					
						<env:object3d width="1.0" height="1.0" depth="1.0"
							x="0" y="0" z="0"
							modelpath="models/goblin/Goblin.j3o"
							materialpath="models/goblin/Goblin.j3m"
							hasLightMaterials="true">
							<env:animation name="Walk" channel="chanA" loop="true" speed="1.3">
								<env:animationcondition>$object.getProperty("status").equals("Walk")</env:animationcondition>
							</env:animation>
							<env:animation name="Idle" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Idle")</env:animationcondition>
							</env:animation>
							
							<env:animation name="Idle" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Sleeping")</env:animationcondition>
							</env:animation>
							<env:animation name="Dance" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Dance")</env:animationcondition>
							</env:animation>
						</env:object3d>
					</env:drawable3d>
					
					<env:drawable3d objecttype="troll" width="1"
						height="1" depth="1">
						
						<env:quad color="#00FF00" width="0.75" height="0.04" depth="0.04" x="0" y="2.0" z="0">
							<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
						</env:quad>
						<env:quad color="#00FFFF" size="awakesize" x="0" y="1.9" z="0">
							<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
						</env:quad>
						<env:quad color="#FFDEAD" size="fedsize" x="0" y="1.85" z="0">
							<env:spatialcontroler classpath="jadex.agentkeeper.view.controller.LifeBarControl"></env:spatialcontroler>
						</env:quad>
						
						<env:object3d width="1" height="1" depth="1"
							x="0" y="0" z="0"
							modelpath="models/orc/Orc.j3o"
							materialpath="models/orc/Orc.j3m"
							hasLightMaterials="true">
							<env:animation name="Walk" channel="chanA" loop="true" speed="2">
								<env:animationcondition>$object.getProperty("status").equals("Walk")</env:animationcondition>
							</env:animation>
							<env:animation name="Idle1" channel="chanA" loop="true" speed="2">
								<env:animationcondition>$object.getProperty("status").equals("Idle")</env:animationcondition>
							</env:animation>
							<env:animation name="Dance" channel="chanA" loop="true" speed="2">
								<env:animationcondition>$object.getProperty("status").equals("Dance")</env:animationcondition>
							</env:animation>
							<env:animation name="Sleep" channel="chanA" loop="true" speed="2">
								<env:animationcondition>$object.getProperty("status").equals("Sleeping")</env:animationcondition>
							</env:animation>
						</env:object3d>
					</env:drawable3d>
					
					<env:drawable3d objecttype="unknown" width="1" height="1" depth="1">
					<env:box color="#FF0000" width="0.9" height="0.2" depth="0.9" ></env:box>

					</env:drawable3d>
				</env:perspective3d>
			</env:perspectives>

			<!-- executor -->
			<env:spaceexecutor class="DeltaTimeExecutor">
				<env:property name="space">$space</env:property>
				<env:property name="tick">true</env:property>
			</env:spaceexecutor>
		</env:envspacetype>
