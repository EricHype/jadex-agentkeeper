					<env:niftyScreen name="default" path="gui/DungeonHud.xml" isStartScreen="false"/>
				
					<env:drawable3d objecttype="static" hasSpaceobject="false"
						width="1" height="1" depth="1">	
						<env:sky skypath="skybox/cave/" west="caveright.jpg" east="caveleft2.jpg" north="caveback.jpg" south="cavefront.jpg" up="caveup2.jpg" down="cavedown.jpg" isSphere="false" />  
						<env:sound3d soundfile="sounds/music/TheSnowQueen.ogg" loop="true" volume="0.1" continuosly="true" positional="false" />
					</env:drawable3d>
					
					
					
					<env:drawable3d objecttype="rock" width="1.0" height="1.0" depth="1.0" isDynamic="false">
						<env:object3d width="0.79998" height="1" depth="0.79998" x="0" y="0" z="0" 
							modelpath="models/tilesets/dirt/Dirt_?.j3o"
							materialpath="models/tilesets/dirt/DirtClicked.j3m">
							<env:drawcondition>$object.getProperty("clicked")</env:drawcondition>
						</env:object3d>
						
						<env:object3d width="0.79998" height="1" depth="0.79998" x="0" y="0" z="0" 
