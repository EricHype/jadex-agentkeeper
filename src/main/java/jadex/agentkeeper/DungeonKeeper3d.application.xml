				<env:avatarmapping componenttype="Imp" objecttype="imp" createcomponent="true"/> 
				<env:avatarmapping componenttype="Goblin" objecttype="goblin" createcomponent="true"/> 
				<env:avatarmapping componenttype="Warlock" objecttype="warlock" createcomponent="true"/>
