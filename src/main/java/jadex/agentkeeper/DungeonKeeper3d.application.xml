						<env:sound3d soundfile="sounds/claim/claimed.ogg" loop="false" volume="0.1" continuosly="false" positional="true"  >
						<env:soundcondition>
							$object.getProperty("status").equals("byImpCreated")
						</env:soundcondition>
						</env:sound3d>
					</env:drawable3d>
					
					<env:drawable3d objecttype="lava" width="1.0" height="1.0" depth="1.0" isDynamic="false">
