						<env:object3d width="0.79998" height="1" depth="0.79998" x="0" y="0" z="0" modelpath="models/tilesets/claimed/Claimedpath.j3o"/>
					</env:drawable3d>
					
					<env:drawable3d objecttype="hatchery" width="1.0" height="1.0" depth="1.0" isDynamic="false">
					<env:property dynamic="true" name="rota" class="IVector3">new Vector3Double(0, 6.2*Math.random() , 0)</env:property>	
						<env:object3d width="0.79998" height="1" depth="0.79998" x="0" y="0" z="0" 
						modelpath="models/tilesets/claimed/Claimedpath.j3o" 
						materialpath="models/buildings/hatchery/HatcheryGround.j3m"/>
					</env:drawable3d>
					
					<env:drawable3d objecttype="hatcherycenter" width="1.0" height="1.0" depth="1.0" isDynamic="false">			
						<env:object3d width="0.79998" height="1" depth="0.79998" x="0" y="0" z="0" 
						modelpath="models/tilesets/claimed/Claimedpath.j3o" 
						materialpath="models/buildings/hatchery/HatcheryGround.j3m" />
						<env:object3d width="0.65" height="0.65" depth="0.65" x="0" y="0" z="0" 
						modelpath="models/buildings/hatchery/farm.j3o" 
						materialpath="models/buildings/hatchery/farm.j3m"
						rotation="rota" />
					</env:drawable3d>
					
					<env:drawable3d objecttype="trainingroom" width="1.0" height="1.0" depth="1.0" isDynamic="false">
					<env:object3d width="0.79998" height="1" depth="0.79998" x="0" y="0" z="0" 
						modelpath="models/tilesets/claimed/Claimedpath.j3o" 
						materialpath="models/buildings/lair/LairGround.j3m"
						 />				
					</env:drawable3d>
					
					<env:drawable3d objecttype="trainingroomcenter" width="1.0" height="1.0" depth="1.0" isDynamic="false">
					<env:property dynamic="true" name="rota" class="IVector3">new Vector3Double(0, 6.2*Math.random() , 0)</env:property>
					
					<env:object3d width="0.79998" height="1" depth="0.79998" x="0" y="0" z="0" 
						modelpath="models/tilesets/claimed/Claimedpath.j3o" 
						materialpath="models/buildings/lair/LairGround.j3m"/>
									
						<env:object3d width="0.3" height="0.3" depth="0.3" x="0" y="0" z="0" 
						modelpath="models/buildings/training/trainingpole.j3o" 
						materialpath="models/buildings/training/trainingpole.j3m" 
						rotation="rota" />			
					</env:drawable3d>


					<env:drawable3d objecttype="imp" width="1" height="1"
						depth="1" isDynamic="true">
						<env:property name="level" dynamic="true">$object.getProperty("level").toString()
						</env:property>
						<env:property name="lifesize" dynamic="true">new
							Vector2Double(((((((Number)$object.getProperty("leben"))).doubleValue()/(((Number)$object.getProperty("maxleben"))).doubleValue()))*0.7),0.05)
						</env:property>
						<env:property name="lifeposition" dynamic="true">new
							Vector2Double((((($object.getProperty("leben")).doubleValue()/($object.getProperty("maxleben")).doubleValue())*0.70)/2)-0.9,-1)
						</env:property>
						<env:object3d width="0.15" height="0.15" depth="0.15"
							x="0" y="0" z="0" modelpath="models/kobold/kobold.j3o"
							hasLightMaterials="true">
							<env:animation name="Walk" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Walk")</env:animationcondition>
							</env:animation>
							<env:animation name="Idle" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Idle")</env:animationcondition>
							</env:animation>
							<env:animation name="Dig" channel="chanA" loop="true" speed="3">
								<env:animationcondition>$object.getProperty("status").equals("Dig")</env:animationcondition>
							</env:animation>
						</env:object3d>
						
						<env:sound3d soundfile="sounds/digslow/Digging?.ogg" numRndFiles="17" loop="false" volume="2.0" continuosly="true" positional="true"  >
						<env:soundcondition>
							$object.getProperty("status").equals("Dig")
						</env:soundcondition>
					</env:sound3d>
					</env:drawable3d>
					
					<env:drawable3d objecttype="warlock" width="1"
						height="1" depth="1" isDynamic="true">
						<env:object3d width="0.08" height="0.08" depth="0.08"
							x="0" y="1.5" z="0"
							modelpath="jadex3d/models/characters/medieval/mage/mage.j3o"
							hasLightMaterials="true">
							<env:animation name="Walk" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Walk")</env:animationcondition>
							</env:animation>
							<env:animation name="Idle" channel="chanA" loop="true">
								<env:animationcondition>$object.getProperty("status").equals("Idle")</env:animationcondition>
							</env:animation>
						</env:object3d>
					</env:drawable3d>

<!-- 					<env:drawable3d objecttype="goblin" width="1" -->
<!-- 						height="1" depth="1"> -->
<!-- 						<env:object3d width="0.15" height="0.15" depth="0.15" -->
<!-- 							x="0" y="1.5" z="0" -->
