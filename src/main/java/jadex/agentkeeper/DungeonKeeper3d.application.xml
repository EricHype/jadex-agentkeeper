							</env:drawcondition>
						</env:effect>
						<env:sound3d soundfile="sounds/rockfalling/RocksFalling?.ogg" numRndFiles="6" loop="false" volume="2.0" continuosly="true" positional="true"  >
						<env:soundcondition>
							$object.getProperty("status").equals("byImpCreated")
